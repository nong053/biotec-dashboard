var pieChart = function(){
	
		$("#chart2").kendoChart({

			title: {
				 text: "สินทรัพย์"
			},
			legend: {
                            position: "bottom"
            },
			series: [{
                            type: "pie",
                            data: [ {
								valueid:"01",
                                category: "สินทรัพย์หมุนเวียน",
                                value: 35
                            }, {
								valueid:"02",
                                category: "สินทรัพย์ไม่หมุนเวียน",
                                value: 65
                            }]
                        }],
                        tooltip: {
                            visible: true,
                            format: "{0}%"

                        },
			
			seriesDefaults: {
				labels: {
					visible: true,
					format: "{0}%"
				}
			}
			
		});


		
		
}//function pie chart end

$("#chart2").click(function(e){
//alert(e.target.id);
