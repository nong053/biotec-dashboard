	 function detailInit(e) {
	
				
								//alert(e.data.Field0);
								if(e.data.Field0==01){
								//alert("hello jquery01");
								$data=$dataJ21;
								pieChart();
								}else{
								alert("hello jquery02");
								$data=$dataJ22;
								}
							
							$("<div/>").kendoGrid({
								
								scrollable: false,
								sortable: true,
								pageable: true,
								 dataBound: function() {
                            this.expandRow(this.tbody.find("tr.k-master-row").first());
                        },
								columns: $titleJ2,
								dataSource: {
								data: $data,
								pageSize: 8
							}
							}).appendTo(e.detailCell);
						 


			// REMOVE COLUMN START
				$("tr.k-detail-row td.k-hierarchy-cell").remove();
			// REMOVE COLUMN END

                } // End Function detailInit



	});

	


